    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.0",
    "express": "4.18.2",
    "express-validator": "6.14.0",
    "hnswlib-node": "1.4.2",
    "jsonwebtoken": "9.0.1",
    "langchain": "0.1.2",
    "lodash": "4.17.21",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "openai": "^4.21.0",
    "pg": "8.11.3",
    "readable-stream": "4.5.2",
